<urn:doc:152#frag-000> a onto:ExplanationFragment ;
    rdfs:label "Preparacao: O professor devera estimular os alunos a \"descobrirem\" e \"relatarem\" os elementos da natureza."@pt ;
    dct:identifier 0 ;
    dct:isPartOf <urn:doc:152> ;
    dct:type "title_or_abstract" ;
