        <urn:doc:329#evid-001-04-centro-circulo> .

<urn:doc:329#frag-002> a onto:ExplanationFragment ;
    rdfs:label "A um sinal do professor voltam a se observar, tentando descobrir qual a alteracao feita."@pt ;
    dct:identifier 2 ;
    dct:isPartOf <urn:doc:329> ;
    dct:type "body" ;
