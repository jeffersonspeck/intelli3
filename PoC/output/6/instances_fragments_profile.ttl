onto:isFragmentOfDocument a owl:ObjectProperty ;
    rdfs:label "isFragmentOfDocument"@en,
        "éFragmentoDeDocumento"@pt ;
    rdfs:comment "Inverse relation of hasFragment (fragment belongs to a document)."@en,
        "Relação inversa de hasFragment (fragmento pertence a um documento)."@pt ;
    rdfs:domain onto:ExplanationFragment ;
    rdfs:range onto:ExplanationDocument ;
    rdfs:subPropertyOf iao:0000136 ;
    owl:inverseOf onto:hasFragment .

