onto:hasProfileVector a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "hasProfileVector"@en,
        "temVetorDePerfil"@pt ;
    rdfs:comment "Links an ExplanationDocument or ExplanationFragment to its MIProfileVector."@en,
